{
  "name": "activity-tracker-api",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/activity-tracker-api",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/activity-tracker-api",
        "commands": [
          {
            "command": "node ace build --production --tsconfig tsconfig.app.json",
            "forwardAllArgs": false
          }
        ],
        "colors": true
      },
      "configurations": {
        "production": {}
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/activity-tracker-api",
        "commands": [
          {
            "command": "node ace serve --watch"
          }
        ],
        "colors": true
      },
      "configurations": {
        "production": {}
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/activity-tracker-api",
        "commands": [
          {
            "command": "node ace test"
          }
        ],
        "colors": true
      },
      "configurations": {
        "production": {}
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/activity-tracker-api/**/*.{ts,js}"]
      }
    }
  },
  "tags": []
}
